      },
      "freeform": {
        "key": "socket:type2_cable"
      },
      "mappings": [
        {
          "if": {
            "and": [
              "socket:type2_cable:output~*",
              "socket:type2_cable:current~*",
              "socket:type2_cable:voltage~*"
            ]
          },
          "then": {
            "pt": "Tomadas Type2 com cabo: <b>{socket:type2_cable}</b> x <b>{socket:type2_cable:output}</b> ({socket:type2_cable:current}A @ {socket:type2_cable:voltage}V)"
          },
          "hideInAnswer": true
        },
        {
          "if": {
            "and": [
              "socket:type2_cable:output~*",
              {
                "or": [
                  "socket:type2_cable:current=",
                  "socket:type2_cable:voltage="
                ]
              }
            ]
          },
          "then": {
            "pt": "Tomadas Type2 com cabo: <b>{socket:type2_cable}</b> x <b>{socket:type2_cable:output}</b>"
          },
          "hideInAnswer": true
        }
      ]
